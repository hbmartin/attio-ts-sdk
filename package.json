{
  "name": "attio-ts-sdk",
  "version": "2.1.0",
  "description": "Type-safe TypeScript SDK for the Attio CRM API with Zod v4 runtime validation",
  "keywords": [
    "attio",
    "crm",
    "sdk",
    "typescript",
    "zod",
    "api",
    "api-client",
    "rest",
    "openapi",
    "type-safe",
    "validation",
    "schema",
    "contacts",
    "sales",
    "attio-api",
    "attio-api-client",
    "attio-rest-api",
    "deals",
    "companies",
    "leads",
    "codegen",
    "customer-data",
    "attio ts",
    "attio js",
    "attio typescript",
    "attio javascript",
    "attio api"
  ],
  "homepage": "https://github.com/hbmartin/attio-ts-sdk",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsdown",
    "typecheck": "tsc --noEmit",
    "dev": "tsdown --watch",
    "lint": "biome check --error-on-warnings",
    "lint:report": "biome check . --reporter=json | jq -r '  .diagnostics[]  | .category' | sort | uniq -c | sort -nr",
    "format": "biome check --write",
    "test": "vitest run --coverage",
    "dupes": "jscpd",
    "prepare": "husky",
    "openapi-ts": "openapi-ts",
    "biome:up": "npx tsx scripts/enable-new-biome-rules.ts --ignore-groups a11y --ignore-rules next --ignore-rules qwik --ignore-rules vue --ignore-rules react --ignore-rules solid --ignore-rules turborepo"
  },
  "author": "Harold Martin <harold.martin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbmartin/attio-ts-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/hbmartin/attio-ts-sdk/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "peerDependencies": {
    "zod": "^4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.3.15",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@hey-api/openapi-ts": "^0.92.3",
    "@types/node": "^24",
    "@vitest/coverage-istanbul": "^4.0.18",
    "husky": "^9.1.7",
    "jscpd": "^4.0.8",
    "publint": "^0.3.17",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "undici-types": "^7.21.0",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "resolutions": {
    "@types/node": "^24"
  },
  "jscpd": {
    "threshold": 0,
    "reporters": [
      "json",
      "console"
    ],
    "ignore": [
      "**/node_modules/**",
      "**/dist/**",
      "**/coverage/**",
      "**/test/**",
      "**/src/generated/**",
      "**/*.json",
      "**/*.test.ts",
      "**/*.md",
      "**/*.yml"
    ],
    "absolute": false
  }
}
