{
  "name": "attio-ts-sdk",
  "version": "0.0.0",
  "description": "TypeScript SDK for Attio CRM based on Zod v4",
  "keywords": [
    "attio",
    "crm",
    "sdk",
    "typescript",
    "zod"
  ],
  "homepage": "https://github.com/hbmartin/attio-ts-sdk",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "browser": "./dist/browser.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "biome check --error-on-warnings",
    "format": "biome format --write",
    "test": "vitest run --coverage",
    "size": "size-limit",
    "dupes": "jscpd"
  },
  "author": "Harold Martin <harold.martin@gmail.com>",
  "repository": "https://github.com/hbmartin/attio-ts-sdk.git",
  "bugs": {
    "url": "https://github.com/hbmartin/attio-ts-sdk/issues"
  },
  "license": "MIT",
  "engines": {
    "node": "24.x"
  },
  "size-limit": [
    {
      "path": "dist/browser.js",
      "limit": "3.5 KB"
    }
  ],
  "peerDependencies": {
    "zod": "^4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@hey-api/openapi-ts": "^0.90.8",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/node": "^24.10.9",
    "@vitest/coverage-istanbul": "^4.0.17",
    "jscpd": "^4.0.7",
    "size-limit": "^8.2.6",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "zod": "^4.3.5"
  },
  "resolutions": {
    "@types/node": "^24"
  },
  "jscpd": {
    "threshold": 0,
    "reporters": [
      "markdown",
      "console"
    ],
    "ignore": [
      "**/node_modules/**",
      "**/dist/**",
      "**/coverage/**",
      "**/tests/**",
      "**/.next/**",
      "**/*.json",
      "**/*.tsx",
      "**/*.test.ts",
      "**/*.css"
    ],
    "absolute": false
  }
}
