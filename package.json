{
  "name": "attio-ts-sdk",
  "version": "1.0.0",
  "description": "Type-safe TypeScript SDK for the Attio CRM API with Zod v4 runtime validation",
  "keywords": [
    "attio",
    "crm",
    "sdk",
    "typescript",
    "zod",
    "api",
    "api-client",
    "rest",
    "openapi",
    "type-safe",
    "validation",
    "schema",
    "contacts",
    "sales"
  ],
  "homepage": "https://github.com/hbmartin/attio-ts-sdk",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "types": "./dist/index.d.mts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsdown",
    "typecheck": "tsc --noEmit",
    "dev": "tsdown --watch",
    "lint": "biome check --error-on-warnings",
    "lint:report": "biome check . --reporter=json | jq -r '  .diagnostics[]  | .category' | sort | uniq -c | sort -nr",
    "format": "biome check --write",
    "test": "vitest run --coverage",
    "dupes": "jscpd",
    "prepare": "husky",
    "openapi-ts": "openapi-ts"
  },
  "author": "Harold Martin <harold.martin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbmartin/attio-ts-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/hbmartin/attio-ts-sdk/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "peerDependencies": {
    "zod": "^4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@hey-api/openapi-ts": "^0.91.1",
    "@types/node": "^24.10.9",
    "@vitest/coverage-istanbul": "^4.0.18",
    "attw": "^1.0.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.8",
    "publint": "^0.3.17",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "resolutions": {
    "@types/node": "^24"
  },
  "jscpd": {
    "threshold": 0,
    "reporters": [
      "markdown",
      "console"
    ],
    "ignore": [
      "**/node_modules/**",
      "**/dist/**",
      "**/coverage/**",
      "**/test/**",
      "**/src/generated/**",
      "**/*.json",
      "**/*.test.ts"
    ],
    "absolute": false
  }
}
